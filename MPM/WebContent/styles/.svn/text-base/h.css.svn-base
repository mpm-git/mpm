.element-invisible {
	position: absolute !important;
	height: 1px;
	width: 1px;
	overflow: hidden;
	clip: rect(1px, 1px, 1px, 1px);
	*clip: rect(1px, 1px, 1px, 1px)
}

.fix:after {
	content: ".";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden
}

* html .fix {
	zoom: 1
}

.fix {
	display: block;
	min-height: 1%
}

